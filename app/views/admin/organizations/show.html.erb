<div class="row">
  <div class="col-md-12" >
    <div class="panel panel-info">
      <div class="panel-heading">
        <h3 class="panel-title"><%= @organization.name %></h3>
      </div>
      <div class="panel-body">
        <div class="row">
          <div class=" col-md-12 col-lg-12 ">
            <table class="table table-user-information">
              <tbody>
                <tr>
                  <td><%= t "name" %></td>
                  <td><%= @organization.name %></td>
                </tr>
                <tr>
                  <td><%= t "create_at" %></td>
                  <td><%= l @organization.created_at, format: :date %></td>
                </tr>
                <tr>
                  <td><%= t "last_update" %></td>
                  <td><%= l @organization.updated_at, format: :date %></td>
                </tr>
                <tr>
                  <td><%= t "phone" %></td>
                  <td><%= @organization.phone %></td>
                </tr>
                <tr>
                  <td><%= t "email" %></td>
                  <td><%= @organization.email %></td>
                </tr>
                <tr>
                  <td><%= t "location" %></td>
                  <td><%= @organization.location %></td>
                </tr>
                <tr>
                  <td><%= t "description" %></td>
                  <td><%= @organization.description %></td>
                </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <div class="panel-footer">
        <%= link_to t("edit"), edit_admin_organization_path(@organization) %>
      </div>
    </div>
  </div>
</div>
