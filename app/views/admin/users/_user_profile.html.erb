<div class="center"><%= image_tag @user.avatar,
  class: "circle-profile" %></div><hr>
<div class="row">
  <div class="col-md-3"></div>
  <div class="col-md-2">
    <strong class="pull-right"><%= t "name" %></strong>
  </div>
  <div class="col-md-6 center"><%= @user.full_name %></div>
  <div class="col-md-1"></div>
</div><hr>
<div class="row">
  <div class="col-md-3"></div>
  <div class="col-md-2">
    <strong class="pull-right"><%= t "email" %></strong>
  </div>
  <div class="col-md-6 center"><%= @user.email %></div>
  <div class="col-md-1"></div>
</div><hr>
<div class="row">
  <div class="col-md-3"></div>
  <div class="col-md-2">
    <strong class="pull-right"><%= t "phone" %></strong>
  </div>
  <div class="col-md-6 center"><%= @user.phone %></div>
  <div class="col-md-1"></div>
</div><hr>
<div class="row">
  <div class="col-md-3"></div>
  <div class="col-md-2">
    <strong class="pull-right"><%= t "organizations.organization" %></strong>
  </div>
  <div class="col-md-6 center">
    <%= render "organization" %>
  </div>
  <div class="col-md-1"></div>
</div><hr>
<div class="row">
  <div class="col-md-3"></div>
  <div class="col-md-2">
    <strong class="pull-right"><%= t "admin_of" %></strong>
  </div>
  <div class="col-md-6 center">
    <%= render "admin_organization"  %>
  </div>
  <div class="col-md-1"></div>
</div>
